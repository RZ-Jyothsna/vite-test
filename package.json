{
  "name": "roanuz-web-v4",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "xdev": "vite",
    "xbuild": "tsc && vite build",
    "xpreview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "dev": "tsx src/server",
    "build": "tsc && npm run build:client && npm run build:server",
    "build:client": "tsc && vite build --ssrManifest --outDir dist/client",
    "build:server": "tsc && vite build --ssr src/entry-server.tsx --outDir dist/server",
    "preview": "cross-env NODE_ENV=production tsx src/server",
    "gql-build": "graphql-codegen --config codegen.ts",
    "gql-watch": "graphql-codegen --watch",
    "editor:gql-build": "graphql-codegen --config src/editor/codegen.ts",
    "editor:gql-watch": "graphql-codegen --watch --config src/editor/codegen.ts"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "express": "^4.18.2",
    "isbot": "^5.0.0",
    "marked": "^12.0.1",
    "react": "^18.3.0-canary-2e470a788-20240214",
    "react-dom": "^18.3.0-canary-2e470a788-20240214",
    "react-router-dom": "^6.22.0",
    "sirv": "^2.0.4"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.2",
    "@parcel/watcher": "^2.4.1",
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.19",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "graphql": "^16.8.1",
    "sass": "^1.71.0",
    "tsx": "^4.7.1",
    "typescript": "^5.2.2",
    "vite": "^5.1.0",
    "vite-plugin-svgr": "3.3.0"
  }
}
